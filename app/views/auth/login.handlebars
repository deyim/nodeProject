<form action="/auth/authenticate" method="POST">
    <div>
        <label>Username:</label>
        <input type="text" name="username"/>
    </div>
    <div>
        <label>Password:</label>
        <input type="password" name="password"/>
    </div>
    <input type="submit" value="login"/>
</form>

{{#if message}}
    <p>{{message}}</p>
{{/if}}