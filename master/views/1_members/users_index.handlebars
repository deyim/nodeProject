<h1>USERS_INDEX</h1>

<form method="get">
    시작날짜: <input type="date" name="startdate"><br>
    끝날짜: <input type="date" name="enddate"><br>
    아이디: <input type="text" name="username" ><br>
    닉네임: <input type="text" name="nickname" ><br>
    이메일수신: <input type="checkbox" name="recEmail" ><br>
    SMS수신: <input type="checkbox" name="recSMS" ><br>
    <input type="submit" value="제출"><br>
</form>


{{#each users}}
    {{this.id}}
    <a href="users/{{this.id}}">{{this.nickname}}</a>
    {{this.createdAt}}
    {{this.loginDate}}
    {{this.loginCnt}}
    <br>
{{/each}}